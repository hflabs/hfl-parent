<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-dependencies</artifactId>
        <version>3.3.6</version>
    </parent>

    <groupId>ru.hflabs</groupId>
    <artifactId>parent</artifactId>
    <version>25.1-SNAPSHOT</version>

    <name>HFLabs Parent POM</name>
    <description>Корневой POM, описывающий версии используемых плагинов и библиотек</description>

    <packaging>pom</packaging>

    <organization>
        <name>Human Factor Labs</name>
        <url>http://www.hflabs.ru</url>
    </organization>

    <licenses>

        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>manual</distribution>
        </license>

    </licenses>

    <scm>
        <url>${project.scm.path}</url>
        <connection>scm:${project.scm.type}:${project.scm.path}</connection>
        <developerConnection>scm:${project.scm.type}:${project.scm.path}</developerConnection>
    </scm>

    <properties>
        <!-- ############################################################################ -->
        <!-- Infrastructure properties -->
        <!-- ############################################################################ -->
        <project.organization.email>info@hflabs.ru</project.organization.email>
        <project.scm.type>git</project.scm.type>
        <project.scm.path>https://github.com/hflabs/hfl-parent-pom.git</project.scm.path>
        <project.scm.revision>${changeSet}</project.scm.revision>

        <!-- ############################################################################ -->
        <!-- Build properties -->
        <!-- ############################################################################ -->
        <j2se.version>21</j2se.version>
        <j2se.debug>true</j2se.debug>
        <j2se.parameters>true</j2se.parameters>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.build.version>3.6.3</maven.build.version>
        <maven.build.version.max.excluded />
        <maven.build.timestamp.format>yyyy-MM-dd_HH.mm.ss</maven.build.timestamp.format>
        <maven.build.settingsFile>${user.home}/.m2/settings.xml</maven.build.settingsFile>

        <!-- ############################################################################ -->
        <!-- Manifest properties -->
        <!-- ############################################################################ -->
        <manifestEntry.Build-Date>${maven.build.timestamp}</manifestEntry.Build-Date>
        <manifestEntry.Implementation-Title>${project.name}</manifestEntry.Implementation-Title>
        <manifestEntry.Implementation-Vendor>${project.organization.name}</manifestEntry.Implementation-Vendor>
        <manifestEntry.Implementation-Vendor-Id>${project.groupId}</manifestEntry.Implementation-Vendor-Id>
        <manifestEntry.Implementation-Version>${project.version} (${project.scm.revision})</manifestEntry.Implementation-Version>

        <!-- ############################################################################ -->
        <!-- Maven properties -->
        <!-- ############################################################################ -->
        <maven.repo>http://maven.hflabs.ru/artifactory</maven.repo>
        <maven.repo.access>local</maven.repo.access>
        <maven.repo.releases.id>hflabs-releases</maven.repo.releases.id>
        <maven.repo.releases>${maven.repo}/libs-releases-${maven.repo.access}/</maven.repo.releases>
        <maven.repo.snapshots.id>hflabs-snapshots</maven.repo.snapshots.id>
        <maven.repo.snapshots>${maven.repo}/libs-snapshots-${maven.repo.access}/</maven.repo.snapshots>
        <maven.repo.snapshots.local>${maven.repo}/libs-snapshots-local/</maven.repo.snapshots.local>
        <maven.repo.snapshots.public>${maven.repo}/libs-snapshots-public/</maven.repo.snapshots.public>

        <skipTests>false</skipTests>
        <maven.test.skip>${skipTests}</maven.test.skip>
        <maven.test.debug>n</maven.test.debug>
        <maven.test.jpda>5010</maven.test.jpda>
        <maven.checkstyle.skip>true</maven.checkstyle.skip>
        <maven.checkstyle.suppressionsLocation />
        <maven.checkstyle.consoleOutput>true</maven.checkstyle.consoleOutput>
        <maven.dependencyVulnerabilities.skip>true</maven.dependencyVulnerabilities.skip>
        <maven.dependencyVulnerabilities.suppressionFile/>
        <maven.war.exploded.extension>exploded.war</maven.war.exploded.extension>
        <maven.war.exploded>${project.build.finalName}_${maven.war.exploded.extension}</maven.war.exploded>
        <maven.compiler.fork>true</maven.compiler.fork>
        <maven.source.skip>false</maven.source.skip>

        <!-- ############################################################################ -->
        <!-- Aspect&ByteCode libraries versions -->
        <!-- ############################################################################ -->
        <jna.version>5.14.0</jna.version>
        <mvel2.version>2.5.2.Final</mvel2.version>
        <cglib.version>3.3.0</cglib.version>

        <!-- ############################################################################ -->
        <!-- Helpers & frameworks -->
        <!-- ############################################################################ -->
        <guava.version>33.2.1-jre</guava.version>
        <streamex.version>0.8.2</streamex.version>
        <joda-time.version>2.12.7</joda-time.version>
        <mapdb.version>3.1.0</mapdb.version>
        <trove.version>3.1.0</trove.version>
        <juniversalchardet.version>2.5.0</juniversalchardet.version>
        <zip4j.version>2.11.5</zip4j.version>

        <!-- ############################################################################ -->
        <!-- Meters & system information -->
        <!-- ############################################################################ -->
        <oshi.version>6.6.1</oshi.version>
        <!-- override to fix https://github.com/micrometer-metrics/micrometer/issues/5193 -->
        <micrometer.version>1.13.4</micrometer.version>
        <micrometer-jvm-extras.version>0.2.2</micrometer-jvm-extras.version>

        <!-- ############################################################################ -->
        <!-- Mappings & parsers versions -->
        <!-- ############################################################################ -->
        <poi.version>5.2.5</poi.version>
        <super-csv.version>2.4.0</super-csv.version>

        <!-- ############################################################################ -->
        <!-- Compression&Serialization versions -->
        <!-- ############################################################################ -->
        <compress-lzf.version>1.1.2</compress-lzf.version>
        <lz4.version>1.8.0</lz4.version>

        <!-- ############################################################################ -->
        <!-- Security versions -->
        <!-- ############################################################################ -->
        <lambdaworks.scrypt.version>1.4.0</lambdaworks.scrypt.version>

        <!-- ############################################################################ -->
        <!-- ANT versions -->
        <!-- ############################################################################ -->
        <ant.version>1.10.14</ant.version>
        <ant-contrib.version>1.0b3</ant-contrib.version>

        <!-- ############################################################################ -->
        <!-- Apache commons version -->
        <!-- ############################################################################ -->
        <commons-cli.version>1.8.0</commons-cli.version>
        <commons-compress.version>1.26.2</commons-compress.version>
        <commons-io.version>2.16.1</commons-io.version>
        <commons-lang.version>2.6</commons-lang.version>
        <commons-dbcp.version>1.4</commons-dbcp.version>
        <commons-digester.version>2.1</commons-digester.version>
        <commons-text.version>1.12.0</commons-text.version>

        <!-- ############################################################################ -->
        <!-- Storage libraries versions -->
        <!-- ############################################################################ -->
        <flyway.version>11.2.0</flyway.version>

        <oracle.groupId>com.oracle</oracle.groupId>
        <oracle.artifactId>ojdbc7</oracle.artifactId>
        <oracle.version>12.1.0.2</oracle.version>
        <oracle.driverClassName>oracle.jdbc.OracleDriver</oracle.driverClassName>
        <oracle.validateQuery>SELECT 1 FROM DUAL</oracle.validateQuery>

        <mariadb.groupId>org.mariadb.jdbc</mariadb.groupId>
        <mariadb.artifactId>mariadb-java-client</mariadb.artifactId>
        <mariadb.version>3.4.0</mariadb.version>
        <mariadb.driverClassName>org.mariadb.jdbc.Driver</mariadb.driverClassName>
        <mariadb.validateQuery>SELECT 1</mariadb.validateQuery>

        <mysql.groupId>com.mysql</mysql.groupId>
        <mysql.artifactId>mysql-connector-j</mysql.artifactId>
        <mysql.version>8.4.0</mysql.version>
        <mysql.driverClassName>com.mysql.jdbc.Driver</mysql.driverClassName>
        <mysql.validateQuery>SELECT 1</mysql.validateQuery>

        <mssql.groupId>com.microsoft.sqlserver</mssql.groupId>
        <mssql.artifactId>mssql-jdbc</mssql.artifactId>
        <mssql.version>12.6.3.jre11</mssql.version>
        <mssql-jdbc.version>${mssql.version}</mssql-jdbc.version>
        <mssql.driverClassName>com.microsoft.sqlserver.jdbc.SQLServerDriver</mssql.driverClassName>
        <mssql.validateQuery>SELECT 1</mssql.validateQuery>

        <postgresql.groupId>org.postgresql</postgresql.groupId>
        <postgresql.artifactId>postgresql</postgresql.artifactId>
        <postgresql.version>42.7.3</postgresql.version>
        <postgresql.driverClassName>org.postgresql.Driver</postgresql.driverClassName>
        <postgresql.validateQuery>SELECT 1</postgresql.validateQuery>

        <hsqldb.groupId>org.hsqldb</hsqldb.groupId>
        <hsqldb.artifactId>hsqldb</hsqldb.artifactId>
        <hsqldb.version>2.7.3</hsqldb.version>
        <hsqldb.driverClassName>org.hsqldb.jdbc.JDBCDriver</hsqldb.driverClassName>
        <hsqldb.validateQuery>SELECT 1 FROM INFORMATION_SCHEMA.SYSTEM_USERS</hsqldb.validateQuery>

        <h2.groupId>com.h2database</h2.groupId>
        <h2.artifactId>h2</h2.artifactId>
        <h2.version>2.1.214</h2.version>
        <h2.driverClassName>org.h2.Driver</h2.driverClassName>
        <h2.validateQuery>SELECT 1</h2.validateQuery>

        <dbf.groupId>com.github.albfernandez</dbf.groupId>
        <dbf.artifactId>javadbf</dbf.artifactId>
        <dbf.version>1.14.1</dbf.version>

        <mongodb.groupId>org.mongodb</mongodb.groupId>
        <mongodb.artifactId>mongodb-driver-sync</mongodb.artifactId>
        <mongodb.version>5.1.1</mongodb.version>

        <!-- ############################################################################ -->
        <!-- SOAP & REST versions -->
        <!-- ############################################################################ -->
        <cxf.version>4.0.5</cxf.version>
        <cxf-xjc.version>4.0.0</cxf-xjc.version>

        <!-- Jaxb versions -->
        <jaxb2.version>0.13.1</jaxb2.version>
        <jaxb2-basics.version>${jaxb2.version}</jaxb2-basics.version>
        <jaxb2-annotate.version>1.1.0</jaxb2-annotate.version>

        <!-- ############################################################################ -->
        <!-- Distributed collections versions -->
        <!-- ############################################################################ -->
        <lucene.version>9.11.1</lucene.version>

        <!-- ############################################################################ -->
        <!-- Web frameworks version -->
        <!-- ############################################################################ -->
        <jsoup.version>1.17.2</jsoup.version>

        <!-- ############################################################################ -->
        <!-- Jakarta (aka javax) specifications libraries version -->
        <!-- ############################################################################ -->
        <jakarta.jws-api.version>2.1.0</jakarta.jws-api.version>
        <jakarta.interceptor-api.version>1.2.3</jakarta.interceptor-api.version>
        <jakarta.resource-api.version>1.7.4</jakarta.resource-api.version>

        <!-- ############################################################################ -->
        <!-- Test frameworks version -->
        <!-- ############################################################################ -->
        <testng.version>7.10.2</testng.version>
        <dbunit.version>2.8.0</dbunit.version>
        <wiremock.version>2.35.2</wiremock.version>

    </properties>

    <build>

        <defaultGoal>install</defaultGoal>

        <pluginManagement>

            <plugins>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-enforcer-plugin</artifactId>
                    <version>${maven-enforcer-plugin.version}</version>
                    <executions>
                        <execution>
                            <id>default-environments</id>
                            <goals>
                                <goal>enforce</goal>
                            </goals>
                            <configuration>
                                <rules>
                                    <requireMavenVersion>
                                        <version>[${maven.build.version},${maven.build.version.max.excluded})</version>
                                    </requireMavenVersion>
                                    <requireJavaVersion>
                                        <version>[${j2se.version},)</version>
                                    </requireJavaVersion>
                                    <!-- use it when upgrading versions for check dependency conflicts -->
                                    <dependencyConvergence />
                                    <banDuplicatePomDependencyVersions/>
                                </rules>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>buildnumber-maven-plugin</artifactId>
                    <version>3.1.0</version>
                    <configuration>
                        <getRevisionOnlyOnce>true</getRevisionOnlyOnce>
                        <revisionOnScmFailure>UNKNOWN</revisionOnScmFailure>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>net.nicoulaj.maven.plugins</groupId>
                    <artifactId>checksum-maven-plugin</artifactId>
                    <version>1.11</version>
                    <configuration>
                        <algorithms>
                            <algorithm>MD5</algorithm>
                        </algorithms>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>${maven-compiler-plugin.version}</version>
                    <configuration>
                        <compilerArgs>
                            <arg>-Xlint:path</arg>
                            <arg>-Xlint:deprecation</arg>
                            <arg>-Xlint:overrides</arg>
                        </compilerArgs>
                        <release>${j2se.version}</release>
                        <encoding>${project.build.sourceEncoding}</encoding>
                        <debug>${j2se.debug}</debug>
                        <parameters>${j2se.parameters}</parameters>
                        <showDeprecation>true</showDeprecation>
                        <showWarnings>true</showWarnings>
                        <fork>${maven.compiler.fork}</fork>
                        <meminitial>128m</meminitial>
                        <maxmem>512m</maxmem>
                    </configuration>
                    <executions>
                        <execution>
                            <id>default-testCompile</id>
                            <goals>
                                <goal>testCompile</goal>
                            </goals>
                            <configuration>
                                <skip>false</skip>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-resources-plugin</artifactId>
                    <version>${maven-resources-plugin.version}</version>
                    <executions>
                        <execution>
                            <id>default-testResources</id>
                            <goals>
                                <goal>testResources</goal>
                            </goals>
                            <configuration>
                                <skip>false</skip>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-remote-resources-plugin</artifactId>
                    <version>3.2.0</version>
                </plugin>

                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>wagon-maven-plugin</artifactId>
                    <version>2.0.2</version>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-source-plugin</artifactId>
                    <version>${maven-source-plugin.version}</version>
                    <executions>
                        <execution>
                            <id>default-sources</id>
                            <phase>verify</phase>
                            <goals>
                                <goal>jar-no-fork</goal>
                                <goal>test-jar-no-fork</goal>
                            </goals>
                            <configuration>
                                <attach>true</attach>
                                <skipSource>${maven.source.skip}</skipSource>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-javadoc-plugin</artifactId>
                    <version>${maven-javadoc-plugin.version}</version>
                    <configuration>
                        <encoding>${project.build.sourceEncoding}</encoding>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-jar-plugin</artifactId>
                    <version>${maven-jar-plugin.version}</version>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <Build-Date>${manifestEntry.Build-Date}</Build-Date>
                                <Implementation-Title>${manifestEntry.Implementation-Title}</Implementation-Title>
                                <Implementation-Vendor>${manifestEntry.Implementation-Vendor}</Implementation-Vendor>
                                <Implementation-Vendor-Id>${manifestEntry.Implementation-Vendor-Id}</Implementation-Vendor-Id>
                                <Implementation-Version>${manifestEntry.Implementation-Version}</Implementation-Version>
                            </manifestEntries>
                        </archive>
                    </configuration>
                    <executions>
                        <execution>
                            <id>default-testJar</id>
                            <goals>
                                <goal>test-jar</goal>
                            </goals>
                            <configuration>
                                <skip>false</skip>
                                <skipIfEmpty>true</skipIfEmpty>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-war-plugin</artifactId>
                    <version>${maven-war-plugin.version}</version>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <Build-Date>${manifestEntry.Build-Date}</Build-Date>
                                <Implementation-Title>${manifestEntry.Implementation-Title}</Implementation-Title>
                                <Implementation-Vendor>${manifestEntry.Implementation-Vendor}</Implementation-Vendor>
                                <Implementation-Vendor-Id>${manifestEntry.Implementation-Vendor-Id}</Implementation-Vendor-Id>
                                <Implementation-Version>${manifestEntry.Implementation-Version}</Implementation-Version>
                            </manifestEntries>
                        </archive>
                        <webappDirectory>${project.build.directory}/${maven.war.exploded}</webappDirectory>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-surefire-plugin</artifactId>
                    <version>${maven-surefire-plugin.version}</version>
                    <configuration>
                        <argLine>-agentlib:jdwp=transport=dt_socket,server=y,suspend=${maven.test.debug},address=${maven.test.jpda}</argLine>
                        <skipTests>${maven.test.skip}</skipTests>
                        <trimStackTrace>false</trimStackTrace>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-failsafe-plugin</artifactId>
                    <version>${maven-failsafe-plugin.version}</version>
                    <configuration>
                        <argLine>-agentlib:jdwp=transport=dt_socket,server=y,suspend=${maven.test.debug},address=${maven.test.jpda}</argLine>
                        <trimStackTrace>false</trimStackTrace>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-checkstyle-plugin</artifactId>
                    <version>3.4.0</version>
                    <dependencies>
                        <dependency>
                            <groupId>ru.hflabs</groupId>
                            <artifactId>parent-resources</artifactId>
                            <version>24.11-SNAPSHOT</version>
                        </dependency>
                    </dependencies>
                    <executions>
                        <execution>
                            <id>default-checkstyle</id>
                            <phase>validate</phase>
                            <goals>
                                <goal>checkstyle</goal>
                            </goals>
                        </execution>
                    </executions>
                    <configuration>
                        <skip>${maven.checkstyle.skip}</skip>
                        <configLocation>checkstyle.xml</configLocation>
                        <failsOnError>true</failsOnError>
                        <linkXRef>false</linkXRef>
                        <suppressionsLocation>${maven.checkstyle.suppressionsLocation}</suppressionsLocation>
                        <consoleOutput>${maven.checkstyle.consoleOutput}</consoleOutput>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.owasp</groupId>
                    <artifactId>dependency-check-maven</artifactId>
                    <version>11.1.0</version>
                    <dependencies>
                        <dependency>
                            <groupId>ru.hflabs</groupId>
                            <artifactId>parent-resources</artifactId>
                            <version>24.11-SNAPSHOT</version>
                        </dependency>
                    </dependencies>
                    <configuration>
                        <skip>${maven.dependencyVulnerabilities.skip}</skip>
                        <failBuildOnCVSS>9</failBuildOnCVSS>
                        <nvdValidForHours>12</nvdValidForHours>
                        <format>HTML</format>
                        <suppressionFiles>
                            <suppressionFile>owasp-suppressions.xml</suppressionFile>
                            <suppressionFile>${maven.dependencyVulnerabilities.suppressionFile}</suppressionFile>
                        </suppressionFiles>
                        <nvdApiKeyEnvironmentVariable>NDV_API_KEY</nvdApiKeyEnvironmentVariable>
                    </configuration>
                    <executions>
                        <execution>
                            <goals>
                                <goal>check</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>sql-maven-plugin</artifactId>
                    <version>3.0.0</version>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-release-plugin</artifactId>
                    <version>3.1.0</version>
                    <configuration>
                        <autoVersionSubmodules>true</autoVersionSubmodules>
                        <updateDependencies>false</updateDependencies>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-archetype-plugin</artifactId>
                    <version>3.2.1</version>
                    <configuration>
                        <settingsFile>${maven.build.settingsFile}</settingsFile>
                    </configuration>
                </plugin>

                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>cobertura-maven-plugin</artifactId>
                    <version>2.7</version>
                </plugin>

            </plugins>

        </pluginManagement>

        <plugins>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-enforcer-plugin</artifactId>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
            </plugin>

        </plugins>

    </build>

    <distributionManagement>
        <repository>
            <id>${maven.repo.releases.id}</id>
            <name>libs-releases</name>
            <url>${maven.repo.releases}</url>
        </repository>
        <snapshotRepository>
            <id>${maven.repo.snapshots.id}</id>
            <name>libs-snapshots</name>
            <url>${maven.repo.snapshots}</url>
            <uniqueVersion>false</uniqueVersion>
        </snapshotRepository>
    </distributionManagement>

    <dependencyManagement>

        <dependencies>

            <!-- Java specifications -->
            <dependency>
                <groupId>jakarta.jws</groupId>
                <artifactId>jakarta.jws-api</artifactId>
                <version>${jakarta.jws-api.version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>jakarta.interceptor</groupId>
                <artifactId>jakarta.interceptor-api</artifactId>
                <version>${jakarta.interceptor-api.version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>jakarta.resource</groupId>
                <artifactId>jakarta.resource-api</artifactId>
                <version>${jakarta.resource-api.version}</version>
            </dependency>

            <!-- Logging -->
            <dependency>
                <groupId>uk.org.lidalia</groupId>
                <artifactId>sysout-over-slf4j</artifactId>
                <version>1.0.2</version>
            </dependency>

            <dependency>
                <groupId>org.logback-extensions</groupId>
                <artifactId>logback-ext-spring</artifactId>
                <version>0.1.5</version>
            </dependency>

            <!-- Aspect&ByteCode libraries -->
            <dependency>
                <groupId>org.mvel</groupId>
                <artifactId>mvel2</artifactId>
                <version>${mvel2.version}</version>
            </dependency>

            <dependency>
                <groupId>cglib</groupId>
                <artifactId>cglib-nodep</artifactId>
                <version>${cglib.version}</version>
            </dependency>

            <dependency>
                <groupId>net.java.dev.jna</groupId>
                <artifactId>jna</artifactId>
                <version>${jna.version}</version>
            </dependency>

            <!-- Helpers & frameworks -->
            <dependency>
                <groupId>com.google.guava</groupId>
                <artifactId>guava</artifactId>
                <version>${guava.version}</version>
            </dependency>

            <dependency>
                <groupId>one.util</groupId>
                <artifactId>streamex</artifactId>
                <version>${streamex.version}</version>
            </dependency>

            <dependency>
                <groupId>joda-time</groupId>
                <artifactId>joda-time</artifactId>
                <version>${joda-time.version}</version>
            </dependency>

            <dependency>
                <groupId>org.mapdb</groupId>
                <artifactId>mapdb</artifactId>
                <version>${mapdb.version}</version>
            </dependency>

            <dependency>
                <groupId>net.sf.trove4j</groupId>
                <artifactId>core</artifactId>
